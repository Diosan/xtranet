$( document ).ready(function() {
        if ( $( "#container" ).height() < $( document ).height() ) {
                $( "#container" ).css({position: 'absolute'});
        }
                           
    	$( ".home_tabs" ).tabs();
    	$( "#post_tabs" ).tabs();
    	$( "#event_date" ).datepicker({
            changeMonth: true,
            changeYear: true,
            dateFormat: "yy-mm-dd"
        });
    	
});
